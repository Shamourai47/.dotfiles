#
#~/.bash_aliases
#

#Convenient aliases
alias e='emacs -nw'
alias la='ls -a'
alias l='ls -l'
alias rmtild='rm *~ && rm .*~'

#git
alias gitst='git status'
alias gitpl='git pull'
alias gitp='git push'
alias gitsl='git shortlog -sn'
alias gitqs='git quick-stats'
alias gitl='git log'

#Color and such
alias gdb='gdb -q'
alias grep='grep --color=auto'
alias ls='ls --color=auto'